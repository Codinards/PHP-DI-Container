{
    "name": "njeaner/di-container",
    "description": "A simple dependency injection container for php projects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jean Nguimfack",
            "email": "nguimjeaner@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "NJContainer\\": "src/",
            "NJContainer\\Tests\\": "tests/"
        },
        "files": ["src/function.php"]
    },
    "require": {
        "php" : "^7.3",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "squizlabs/php_codesniffer": "^3.5",
        "friendsofphp/php-cs-fixer": "^2.16",
        "php-coveralls/php-coveralls": "^2.2"
    }
}
